    </Reference>
    <Reference Include="ogr_csharp">
      <HintPath>..\dll\ogr_csharp.dll</HintPath>
    </Reference>
    <Reference Include="osr_csharp">
      <HintPath>..\dll\osr_csharp.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>